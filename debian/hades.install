conf/*.target lib/systemd/system/
conf/*.timer lib/systemd/system/
conf/config.py => etc/hades/config/__init__.py
conf/hades.busconfig => etc/dbus-1/system.d/de.agdsn.hades.conf
conf/hades.default => etc/default/hades
