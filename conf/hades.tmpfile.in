# This file can be replace by RuntimeDirectory settings in the unit files
# starting with systemd 233 (not available in Stretch), because the
# RuntimeDirectoryPreserve setting was added there.
d @pkgrunstatedir@/agent            2750    @AGENT_USER@ @AGENT_GROUP@
d @pkgrunstatedir@/auth-dhcp        2750    @AUTH_DHCP_USER@ @AUTH_DHCP_GROUP@
d @pkgrunstatedir@/auth-netns       0750    root root
d @pkgrunstatedir@/auth-dns         2750    @AUTH_DNS_USER@ @AUTH_DNS_GROUP@
d @pkgrunstatedir@/auth-vrrp        0750    root root
d @pkgrunstatedir@/database         2751    @DATABASE_USER@ @DATABASE_GROUP@
d @pkgrunstatedir@/database/conf    2750    @DATABASE_USER@ @DATABASE_GROUP@
d @pkgrunstatedir@/deputy           0750    root root
d @pkgrunstatedir@/radius           2750    @RADIUS_USER@ @RADIUS_GROUP@
d @pkgrunstatedir@/radius-vrrp      0750    root root
d @pkgrunstatedir@/root-netns       0750    root root
d @pkgrunstatedir@/unauth-dns       2750    @UNAUTH_DNS_USER@ @UNAUTH_DNS_GROUP@
d @pkgrunstatedir@/unauth-http      2750    @PORTAL_USER@ @PORTAL_GROUP@
d @pkgrunstatedir@/unauth-netns     0750    root root
d @pkgrunstatedir@/unauth-portal    2750    @PORTAL_USER@ @PORTAL_GROUP@
d @pkgrunstatedir@/unauth-vrrp      0750    root root
