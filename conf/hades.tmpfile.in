# This file handles directories not configurable via RuntimeDirectory settings in unit files,
# e.g. because the process drops user/group
# privileges.
# dnsmasq drops privileges, unit run as root
d @pkgrunstatedir@/auth-dhcp        2749    @AUTH_DHCP_USER@ @AUTH_DHCP_GROUP@
# auth-dns/ shared by hades-auth-{pristine,alternative}-dns
d @pkgrunstatedir@/auth-dns         2750    @AUTH_DNS_USER@ @AUTH_DNS_GROUP@
# dnsmasq drops privileges, unit run as root
d @pkgrunstatedir@/unauth-dns       2750    @UNAUTH_DNS_USER@ @UNAUTH_DNS_GROUP@
# nginx drops privileges, unit run as root
d @pkgrunstatedir@/unauth-http      2750    @PORTAL_USER@ @PORTAL_GROUP@
